mkdir elastic

wget https://artifacts.elastic.co/downloads/elasticsearch/elasticsearch-8.11.0-linux-x86_64.tar.gz
tar -xvzf elasticsearch-8.11.0-linux-x86_64.tar.gz -C elastic

wget https://artifacts.elastic.co/downloads/kibana/kibana-8.11.0-linux-x86_64.tar.gz
tar -xvzf kibana-8.11.0-linux-x86_64.tar.gz -C elastic

wget https://artifacts.elastic.co/downloads/logstash/logstash-8.11.0-linux-x86_64.tar.gz
tar -xvzf logstash-8.11.0-linux-x86_64.tar.gz -C elastic

